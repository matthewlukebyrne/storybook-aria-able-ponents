{
  "name": "storybook-aria-able-codebase-backend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/cli": "5.3.13",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "actions": "^1.3.0",
    "init": "^0.1.2",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sb": "^6.3.6",
    "styled-components": "^5.3.0",
    "styled-components-modifiers": "^1.2.5",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-backgrounds": "^5.3.14",
    "@storybook/addon-contexts": "^5.3.14",
    "@storybook/addon-docs": "^5.3.14",
    "@storybook/addon-essentials": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/node-logger": "^5.3.13",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^5.3.13"
  }
}
